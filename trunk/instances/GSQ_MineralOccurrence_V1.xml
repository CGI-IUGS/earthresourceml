				<gml:Point srsName="EPSG:28354">
					<!-- Easting and northing come from SGF_LOCATIONS.EASTING and SGF_LOCATIONS.NORTHING. Values are in GDA94 -->
					<gml:pos>491720 7582016</gml:pos>

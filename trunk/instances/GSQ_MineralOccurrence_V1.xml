			<gsml:observationMethod>
				<gsml:CGI_TermValue>
					<!-- Observation method comes from SGF_SITES.LOC_METHOD_CODE. Lookup table is CPF_LOCATION_METHODS-->
					<gsml:value codeSpace="urn:cgi:classifierScheme:GSQ:CaptureMethod">Global Positioning System (AMG-66)</gsml:value>
				</gsml:CGI_TermValue>

	<mo:resourceExtraction>
		<!-- GSQ MINOCC stores production figures in SGF_PRODUCTION_ORE AND SGF_PRODUCTION_COMMODITES -->
		<mo:MiningActivity gml:id="id_ma01">
			<mo:activityDuration>
				<gml:TimePeriod>
					<gml:begin>
						<!-- GSQ MINOCC stores start date for production period in SGF_PRODUCTION_ORE.PROD_START_DATE -->
						<gml:TimeInstant>
							<gml:timePosition>01-Jul-2001</gml:timePosition>
						</gml:TimeInstant>
					</gml:begin>
					<gml:end>
						<!-- GSQ MINOCC stores end date for production period in SGF_PRODUCTION_ORE.PROD_END_DATE -->
						<gml:TimeInstant>
							<gml:timePosition>30-Jun-2002</gml:timePosition>
						</gml:TimeInstant>
					</gml:end>
				</gml:TimePeriod>
			</mo:activityDuration>
			<!-- GSQ MINOCC does not store activity type as a specific field. Default to unspecified -->
			<mo:activityType>unspecified</mo:activityType>
			<!-- GSQ MINOCC stores ore produced in SGF_PRODUCTION_ORE.ORE_AMOUNT. Units are tonnes. Ore amount will be null where it has not been reported. How will this be handled.  -->
			<mo:oreProcessed>
